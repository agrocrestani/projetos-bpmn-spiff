      <bpmn:extensionElements>
        <spiffworkflow:instructionsForEndUser>Escolha a filial de origem onde o ativo está localizado e a filial de destino para onde o ativo será transferido.</spiffworkflow:instructionsForEndUser>
        <spiffworkflow:properties>
          <spiffworkflow:property name="formJsonSchemaFilename" value="definir-filial-origem-destino-schema.json" />
          <spiffworkflow:property name="formUiSchemaFilename" value="definir-filial-origem-destino-uischema.json" />
        </spiffworkflow:properties>
        <spiffworkflow:preScript>n_filiais = []

for filial in filiais:
    n_filiais.append( {
        "value": filial['BPLId'],
        "label": filial['AliasName'],
    })

del(filiais)
del(filial)</spiffworkflow:preScript>
        <spiffworkflow:postScript>error = None

if BPLIdDestino == BPLIdOrigem:
    error = "Não é possível transferir um ativo para a mesma filial de origem."
</spiffworkflow:postScript>
