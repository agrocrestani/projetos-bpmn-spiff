{
  "title": "Dropdown list",
  "description": "A dropdown list with options pulled form existing Task Data. IMPORTANT - Add 'fruits' to Task Data before using this component!!!",
  "type": "object",
  "properties": {
    "due": {
      "title": "",
      "description": "",
      "type": "object",
      "properties": {
        "numero_due": {
          "type": "string",
          "title": "Numero DUE"
        },
        "data_embarque": {
          "type": "string",
          "format": "date",
          "title": "Data Embarque"
        },
        "data_averbacao": {
          "type": "string",
          "format": "date",
          "title": "Data Averbação"
        },
        "entidade": {
          "type": "string",
          "title": "Entidade",
          "anyOf": [
        "options_from_task_data_var:fruits"
      ]

        },
        "naturezaDeclaracao": {
          "type": "string",
          "title": "Natureza da Declaração",
          "anyOf": [
        "options_from_task_data_var:fruits"
      ]
        },
        "tipoConhecimento": {
          "type": "string",
          "title": "Tipo do Conhecimento"
        },
        "tipoDeclaracao": {
          "type": "string",
          "title": "Tipo Declaração"
        }
      }
    },
    "favoriteFruit": {
      "title": "Select your favorite fruit",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:fruits"
      ]
    }
  }
}