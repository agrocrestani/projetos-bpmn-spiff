{
  "title": "Solicitação de Compras",
  "description": "Solicitação de compras de items para estoque",
  "type": "object",
    "required": [
      "BPLId",
      "Department",
      "U_RSD_Prioridade",
      "U_RSD_Aplicacao",
      "OcrCode",
      "OcrCode3",
      "WhsCode",
      "MainUsage",
      "solicitacao"
    ],
  "properties": {
    "BPLId": {
      "title": "Filial",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_filiais"
      ]
    },
    "Department": {
      "title": "Departamento",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_departamentos"
      ]
    },
    "U_RSD_Prioridade": {
      "title": "Prioridade",
      "type": "string",
      "anyOf": [
        {
          "type": "string",
          "enum": [
            "1"
          ],
          "title": "Normal"
        },
        {
          "type": "string",
          "enum": [
            "2"
          ],
          "title": "Urgente"
        },
        {
          "type": "string",
          "enum": [
            "3"
          ],
          "title": "Fora Processo"
        }
      ]
    },
    "U_RSD_Aplicacao": {
      "title": "Tipo Requisicao",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_tipo_requisicao"
      ]
    },
    "OcrCode": {
      "title": "Centro de Custo",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_centros_de_custo"
      ]
    },
    "OcrCode2": {
      "title": "Cultura",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_culturas"
      ]
    },
    "OcrCode3": {
      "title": "Safra",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_safras"
      ]
    },
    "WhsCode": {
      "title": "Deposito",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_almoxarifados"
      ]
    },
    "MainUsage": {
      "title": "Utilizacao",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_utilizacoes"
      ]
    },
    "Comments": {
      "type": "string",
      "title": "Observações"
    },
    "qtdeColaboradores": {
      "title": "Quantidade de colaboradores",
      "type": "number"
    },
    "periodo": {
      "title": "Periodo",
      "type": "string",
      "anyOf": [
        {
          "type": "string",
          "enum": [
            "1"
          ],
          "title": "0 - "
        },
        {
          "type": "string",
          "enum": [
            "2"
          ],
          "title": "Urgente"
        },
        {
          "type": "string",
          "enum": [
            "3"
          ],
          "title": "Fora Processo"
        }
      ]
    }
  }
}