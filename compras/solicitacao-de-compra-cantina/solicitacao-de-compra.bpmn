            "result": {
                "ItemCode": ItemCode, 
                "ItemName": ItemName, 
                "InvntryUom": InvntryUom if InvntryUom != 'null' else None,
                "QtdeSugerida": QtdeSugerida
                }
            }
        )

typeahead = {"items_cantina": items_cantina}

del(connection)</bpmn:script>
