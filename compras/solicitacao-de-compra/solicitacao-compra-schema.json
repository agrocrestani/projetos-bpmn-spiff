  "properties": {
    "BPLId": {
      "title": "Filial",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_filiais"
      ]
    },
    "Department": {
      "title": "Departamento",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:departamentos"
      ]
    },
    "U_RSD_Prioridade": {
      "title": "Prioridade",
      "type": "string",
       "anyOf": [
        {
          "type": "string",
          "enum": [
            "1"
          ],
          "title": "Normal"
        },
        {
          "type": "string",
          "enum": [
            "2"
          ],
          "title": "Urgente"
        },
        {
          "type": "string",
          "enum": [
            "3"
          ],
          "title": "Fora Processo"
        }
      ]
    },
    "OcrCode": {
      "title": "Centro de Custo",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:centros_de_custo"
      ]
    },
    "OcrCode2": {
      "title": "Cultura",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:culturas"
      ]
    },
    "OcrCode3": {
      "title": "Safra",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:safras"
      ]
    },
    "WhsCode": {
      "title": "Safra",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:safras"
      ]
    },
    "MainUsage": {
      "title": "Safra",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:utilizacoes"
      ]
    }
  }
}