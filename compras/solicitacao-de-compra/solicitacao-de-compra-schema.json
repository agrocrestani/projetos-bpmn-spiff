{
  "title": "Dropdown list",
  "description": "A dropdown list with options pulled form existing Task Data. IMPORTANT - Add 'fruits' to Task Data before using this component!!!",
  "type": "object",
  "properties": {
    "filialSolicitacao": {
      "title": "Filial",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_filiais"
      ]
    },
    "departamentoSolicitacao": {
      "title": "Departamento",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:departamentos"
      ]
    },
    "tipoRequisicaoSolicitacao": {
      "title": "Tipo de Requisição",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:tipo_requisicao"
      ]
    },
    "prioridadeSolicitacao": {
      "title": "Prioridade",
      "type": "string",
      "anyOf": [
        {
          "type": "string",
          "enum": [
            "1"
          ],
          "title": "Normal"
        },
        {
          "type": "string",
          "enum": [
            "2"
          ],
          "title": "Urgente"
        },
        {
          "type": "string",
          "enum": [
            "3"
          ],
          "title": "Fora Processo"
        }
      ]
    },
    "centroDeCustoSolicitacao": {
      "title": "Centro de Custo",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:centroDeCusto"
      ]
    },
    "safraSolicitacao": {
      "title": "Centro de Custo",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:safra"
      ]
    }
  }
}