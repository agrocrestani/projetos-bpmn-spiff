{
  "title": Contabilização",
  "description": "A dropdown list with options pulled form existing Task Data. IMPORTANT - Add 'fruits' to Task Data before using this component!!!",
  "type": "object",
  "properties": {
    "datalacto": {
      "title": "Data de Lançamento",
      "type": "string",
      "format": "date"
    },
    "safra": {
      "title": "Safra",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_safras"
      ]
    },
    "cultura": {
      "title": "Cultura",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_culturas"
      ]
    },
    "file": {
      "type": "string",
      "format": "data-url",
      "title": "Selecione o arquivo"
    }
  }
}