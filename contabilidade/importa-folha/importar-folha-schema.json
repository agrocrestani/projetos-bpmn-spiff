{
  "title": "Contabilização da Folha de Pagamento",
  "type": "object",
  "required": [
          "datalacto",
          "safra",
          "file"
        ],
  "properties": {
    "datalacto": {
      "title": "Data de Lançamento",
      "type": "string",
      "format": "date"
    },
    "safra": {
      "title": "Safra",
      "type": "string",
      "anyOf": {{n_safras|tojson}}
    },
    "cultura": {
      "title": "Cultura",
      "type": "string",
      "anyOf": {{n_culturas|tojson}}
    },
    "file": {
      "type": "string",
      "format": "data-url",
      "title": "Selecione o arquivo"
    }
  }
}