      <bpmn:script>from connectordb.hana import Hana

connection = {
    "address": "172.20.1.4",
    "port": "30015",
    "user": "SYSTEM",
    "password": "9Ab63^Op33"
}

baseSap = get_secret('baseSap')

query = f"""
            SELECT
                o."WhsCode",
                o."WhsName",
                o."U_AC_IdFarmBox",
                o."U_AC_Id_Solinftec"
            FROM
                {baseSap}.owhs o
            WHERE
                o."BPLid" = {BPLId}
                AND o."WhsCode" = '{WhsCode}'
          """

