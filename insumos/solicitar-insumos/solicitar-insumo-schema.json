{
  "title": "Dropdown list",
  "description": "A dropdown list with options pulled form existing Task Data. IMPORTANT - Add 'fruits' to Task Data before using this component!!!",
  "type": "object",
  "required": [
        "solicitacao"
      ],
  "properties": {
    "solicitacao": {
      "type": "array",
      "title": "Items",
      "description": "Selecione os ite"
      "items": {
        "type": "object",
        "required": [
          "insumo",
          "quantidade"
        ],
        "properties": {
          "insumo": {
            "type": "string",
            "title": "Selecione o Insumo"
          },
          "quantidade": {
            "type": "number",
            "title": "Quantidade"
          }
        }
      }
    },
    "unidade": {
      "title": "Selecione a unidade",
      "type": "string",
      "anyOf": [
        "options_from_task_data_var:n_filiais"
      ]
    }
  }
}