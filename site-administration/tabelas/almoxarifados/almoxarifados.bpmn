      <bpmn:incoming>Flow_1l7jori</bpmn:incoming>
      <bpmn:outgoing>Flow_1pbmx0d</bpmn:outgoing>
      <bpmn:script>from connectordb.hana import Hana

baseSap = get_secret('baseSap')

connection = {
    "address": get_secret('hanaAddress'),
    "port": get_secret('hanaPort'),
    "user": get_secret('hanaUser'),
    "password": get_secret('hanaPassword')
}

query = f"""
      SELECT 
        o."WhsCode", 
        o."WhsName", 
        o."BPLid" 
      FROM 
        {baseSap}.owhs o 
      WHERE 
        o."WhsCode" LIKE '01.%'
        OR o."WhsCode" LIKE '02.%'
        OR o."WhsCode" LIKE '05.%'
        OR o."WhsCode" LIKE '06.%'
        OR o."WhsCode" LIKE '07.%'
        OR o."WhsCode" LIKE '08.%'
        OR o."WhsCode" LIKE '09.%'
        OR o."WhsCode" LIKE '10.%'
        OR o."WhsCode" LIKE '1%'
      ORDER BY 
        o."WhsCode" 
          """

# rsp = json.loads(Hana(connection).selectDb(query))
rsp = Hana(connection).selectDb(query)
del(connection)</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:sequenceFlow id="Flow_1l9e5wy" sourceRef="Activity_1iixjle" targetRef="EndEvent_1" />
    <bpmn:scriptTask id="Activity_1iixjle" name="Parse Almoxarifados">
      <bpmn:incoming>Flow_1pbmx0d</bpmn:incoming>
      <bpmn:outgoing>Flow_1l9e5wy</bpmn:outgoing>
      <bpmn:dataOutputAssociation id="DataOutputAssociation_1721ddj">
        <bpmn:targetRef>DataStoreReference_0jau633</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
      <bpmn:script>calmoxarifados = []

for r in rsp:
    WhsCode = str(r['whscode'])
    WhsName = str(r['whsname'])
    BPLid = str(r['bplid'])
    calmoxarifados.append(
        {
            "WhsCode": WhsCode, 
            "WhsName": WhsName, 
            "BPLid": BPLid
        }
    )

almoxarifados = calmoxarifados

del(rsp)
del(calmoxarifados)</bpmn:script>
    </bpmn:scriptTask>
