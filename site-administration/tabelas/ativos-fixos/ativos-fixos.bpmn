      <bpmn:script>from connectordb.hana import Hana

connection = {
    "address": "172.20.1.4",
    "port": "30015",
    "user": "SYSTEM",
    "password": "9Ab63^Op33"
}

baseSap = get_secret('baseSap')

query = f"""
            SELECT  
                t0."ItemCode" ,
                t0."ItemName" ,
                t0.U_RSD_FILIAL ,
                t1."AliasName" 
            FROM 
                {baseSap}.OITM t0
                INNER JOIN {baseSap}.OBPL t1 ON t1."BPLId" = t0.U_RSD_FILIAL 
            WHERE t0."ItemType" = 'F' AND t0.U_RSD_FILIAL = {BPLIdOrigem}
        """

query = f"""
            SELECT  
                t0."ItemCode" ,
                t0."ItemName" ,
                t0.U_RSD_FILIAL ,
                t1."AliasName" 
            FROM 
                {baseSap}.OITM t0
                INNER JOIN {baseSap}.OBPL t1 ON t1."BPLId" = t0.U_RSD_FILIAL 
            WHERE t0."ItemType" = 'F' AND t0.U_RSD_FILIAL = 1
        """

rsp = Hana(connection).selectDb(query)

del(connection)
ativos_fixos = []

for r in rsp:
    ItemCode = str(r['itemcode'])
    ItemName = str(r['itemname'])
    U_RSD_FILIAL = str(r['u_rsd_filial'])
    AliasName = str(r['aliasname'])
    ativos_fixos.append(
        {
            "search_term": ItemName, 
            "result": {
                "ItemCode": ItemCode, 
                "ItemName": ItemName,
                "U_RSD_FILIAL": U_RSD_FILIAL,
                "AliasName": AliasName
                }
            }
        )

del(ItemCode)
del(ItemName)
del(U_RSD_FILIAL)
del(AliasName)
del(r)
del(rsp)


typeahead = {f"ativos_fixos_{BPLIdOrigem}": ativos_fixos}

qtdItens = len(ativos_fixos)

del(ativos_fixos)
</bpmn:script>
