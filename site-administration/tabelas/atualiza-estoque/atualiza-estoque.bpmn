        <spiffworkflow:preScript>table = "sap.\"vw_est_insumos\""

schema = {
  "columns": [
    "\"ItemCode\"",
    "\"ItemName\"",
    "\"OnHand\""
  ]
}</spiffworkflow:preScript>
        <spiffworkflow:postScript>estoque_de_insumos = []

for result in rsp['body']:
    name = result[1]
    code = result[0]
    onhand = result[2]
    estoque_de_insumos.append(
        {
            "search_term": name, 
            "result": {
                "name": name, 
                "code": code, 
                "onhand": onhand.strip()
                }
            }
        )

del(rsp)</spiffworkflow:postScript>
