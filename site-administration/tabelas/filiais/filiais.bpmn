      </bpmn:dataOutputAssociation>
      <bpmn:script>cfiliais = []

for r in rsp:
    BPLId = str(r['bplid'])
    BPLName = str(r['bplname'])
    TaxIdNum = str(r['taxidnum'])
    EntidadeId = str(r['entidadeid'])
    AliasName = str(r['aliasname'])
    cfiliais.append(
        {
            "BPLId": BPLId, 
            "BPLName": BPLName, 
            "TaxIdNum": TaxIdNum, 
            "EntidadeId": EntidadeId
            "AliasName": AliasName
        }
    )

filiais = cfiliais

del(rsp)
del(cfiliais)

</bpmn:script>
