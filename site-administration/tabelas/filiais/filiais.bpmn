      <bpmn:script>from connectordb.hana import Hana

connection = {
    "address": "172.20.1.4",
    "port": "30015",
    "user": "SYSTEM",
    "password": "9Ab63^Op33"
}

query = """
          SELECT 
            to_alphanum(o."BPLId") "BPLId",
            o."BPLName",
            o."TaxIdNum",
            to_alphanum(e.ID "EntidadeId") "EntidadeId",
            o."AliasName" 
          FROM
            SBO_CRESTANI_PRD.OBPL o
          INNER JOIN SBO_CRESTANI_PRD."Entidade" e ON
            O."TaxIdNum" = e.CNPJ
        """

rsp = json.loads(Hana(connection).selectDb(query))

del(connection)</bpmn:script>
    </bpmn:scriptTask>
