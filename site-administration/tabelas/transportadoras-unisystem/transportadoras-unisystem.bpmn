    <bpmn:scriptTask id="Activity_0dyhbdy" name="Fetch Transportadora">
      <bpmn:incoming>Flow_0tn4ed3</bpmn:incoming>
      <bpmn:outgoing>Flow_15602ew</bpmn:outgoing>
      <bpmn:script>from connectordb.oracle import Oracle
from datetime import datetime

connection = {
    "dsn": get_secret('oracleDsn'),
    "user": get_secret('oracleUser'),
    "password": get_secret('oraclePassword')
}

query_transp = f"SELECT * FROM AC_VW_TRANSPORTADORA"
rsp_transp = Oracle(connection).selectDb(query_transp)
transportadoras = []

for t in rsp_transp:
    codigo_transp = str(t['seq_pla_endereco']) 
    nome_transp = f"{str(t['nome'])} | {str(t['cnpj'])}"
    transportadoras.append(
        {
            "search_term": nome_transp,
            "result": {
                "codigo_transp": codigo_transp,
                "nome_transp": nome_transp
            }
        }
    )

del(connection)
del(query_transp)
del(rsp_transp)</bpmn:script>
    </bpmn:scriptTask>
