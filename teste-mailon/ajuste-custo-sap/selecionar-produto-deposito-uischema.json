{
  "type": "VerticalLayout",
  "elements": [
    {
      "type": "HorizontalLayout",
      "elements": [
        {
          "type": "Control",
          "scope": "#/properties/dataInicio",
          "options": {
            "ui:emptyValue": "",
            "ui:placeholder": "Data de Início",
            "ui:widget": "date"
          }
        },
        {
          "type": "Control",
          "scope": "#/properties/dataFinal",
          "options": {
            "ui:emptyValue": "",
            "ui:placeholder": "Data Final",
            "ui:widget": "date"
          }
        }
      ]
    },
    {
      "type": "Control",
      "scope": "#/properties/produto",
      "options": {
        "ui:emptyValue": "",
        "ui:placeholder": "Selecione um produto",
        "ui:autocomplete": "family-name",
        "ui:enableMarkdownInDescription": true
      }
    },
    {
      "type": "Control",
      "scope": "#/properties/deposito",
      "options": {
        "ui:emptyValue": "",
        "ui:placeholder": "Selecione um deposito",
        "ui:autocomplete": "family-name",
        "ui:enableMarkdownInDescription": true
      }
    }
  ],
  "properties": {
    "dataInicio": {
      "type": "string",
      "title": "Data de Início"
    },
    "dataFinal": {
      "type": "string",
      "title": "Data Final"
    },
    "produto": {
      "type": "string",
      "title": "Produto"
    },
    "deposito": {
      "type": "string",
      "title": "Deposito"
    }
  }
}
