        <bpmn:targetRef>DataStoreReference_0egoqnt</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
      <bpmn:script>typeahead = {"produto_com_movimento": produto_com_movimento}</bpmn:script>
    </bpmn:scriptTask>
