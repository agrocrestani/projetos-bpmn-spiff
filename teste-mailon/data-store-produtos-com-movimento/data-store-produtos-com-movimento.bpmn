    <bpmn:scriptTask id="BUSCAR_PRODUTO" name="Buscar Produtos">
      <bpmn:incoming>Flow_17db3yp</bpmn:incoming>
      <bpmn:outgoing>Flow_06y0iod</bpmn:outgoing>
      <bpmn:script>from connectordb.hana import Hana

connection = {
    "address": "172.20.1.4",
    "port": "30015",
    "user": "SYSTEM",
    "password": "9Ab63^Op33"
}

baseSap = get_secret('baseSap')

query_produtos = f"""
                    SELECT DISTINCT 
                        t0."ItemCode" ,
                        t1."ItemName" 
                    FROM 
                        {baseSap}.OITW t0
                        INNER JOIN {baseSap}.OITM t1 ON t1."ItemCode" = t0."ItemCode"
                    WHERE t0."AvgPrice" &gt; 0
                    ORDER BY t0."ItemCode"
                """

rsp = Hana(connection).selectDb(query_produtos)

produto_com_movimento = []

for r in rsp:
    ItemCode = str(r['itemcode'])
    ItemName = str(r['itemname'])
    produto_com_movimento.append(
        {
            "search_term": ItemName, 
            "result": {
                "ItemCode": ItemCode, 
                "ItemName": ItemName
                }
            }
        )




del(connection)</bpmn:script>
    </bpmn:scriptTask>
