    "address": "172.20.1.4",
    "port": "30015",
    "user": "SYSTEM",
    "password": "9Ab63^Op33"
}

query = f"""
            SELECT DISTINCT 
                CAST(t0."ItemCode" AS VARCHAR(20)) AS "itemcode" ,
                CAST(t1."ItemName" AS VARCHAR(200)) AS "itemname"
            FROM 
                {baseSap}.OITW t0
                INNER JOIN {baseSap}.OITM t1 ON t1."ItemCode" = t0."ItemCode"
            WHERE t0."AvgPrice" &gt; 0
        """

rsp = Hana(connection).selectDb(query)

del(connection)</bpmn:script>
